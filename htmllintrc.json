{
  "attr-bans": ["on*"],
  "attr-name-ignore-regex": false,
  "attr-name-style": "dash",
  "attr-new-line": false,
  "attr-no-dup": true,
  "attr-no-unsafe-char": true,
  "attr-order": false,
  "attr-quote-style": "double",
  "attr-req-value": false,
  "attr-validate": true,

  "class-no-dup": true,
  "class-style": "dash",

  "doctype-first": "smart",
  "doctype-html5": false,

  "fig-req-figcaption": true,
  "focusable-tabindex-style": true,

  "head-req-title": true,
  "head-valid-content-model": true,

  "href-style": false,

  "html-req-lang": false,

  "id-class-ignore-regex": false,
  "id-class-style": "dash",
  "id-no-dup": true,

  "img-req-alt": true,
  "img-req-src": true,

  "indent-style": "spaces",
  "indent-width": 2,
  "indent-width-cont": true,

  "input-radio-req-name": false,
  "input-req-label": true,

  "label-req-for": false,

  "lang-style": "case",

  "line-end-style": "lf",
  "line-max-len": 120,
  "line-max-len-ignore-regex": "href|src",
  "line-no-trailing-whitespace": true,

  "maxerr": false,

  "raw-ignore-regex": "[ ]*<%[^]*?%>|<%%",

  "spec-char-escape": true,

  "table-req-caption": false,
  "table-req-header": false,

  "tag-bans": ["style", "b"],
  "tag-close": true,
  "tag-name-lowercase": true,
  "tag-name-match": true,
  "tag-self-close": false,

  "text-ignore-regex": false,

  "title-max-len": false,
  "title-no-dup": true
}
